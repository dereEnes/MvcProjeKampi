@using Entity.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<h2>Hakkımda veri kümesi</h2>
<table class="table table-primary table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>Hakkımda Başlığı</th>
            <th>Aktif</th>
            <th>Pasif</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AboutId</td>
                <td>Başlık</td>
                <td><a href="#" class="btn btn-success">Aktif</a></td>
                <td><a href="#" class="btn btn-warning">Pasif</a></td>
            </tr>
        }
    </tbody>
    <tfoot>
       Başlıklara ait bilgiler
    </tfoot>
</table>
<br />
@Html.Action("AboutPartial","About")